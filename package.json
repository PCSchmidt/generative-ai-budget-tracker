{
  "name": "generative-ai-budget-tracker",
  "version": "1.0.0",
  "description": "Smart expense tracking with AI-powered insights",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "lint": "eslint .",
    "build": "expo build",
    "vercel-build": "npm run build:html",
    "build:html": "node -e \"const fs=require('fs'),path=require('path'); fs.copyFileSync('web-frontend/index.html','index.html'); fs.copyFileSync('priority4_dashboard.html','priority4.html'); console.log('✅ Copied web-frontend/index.html to index.html'); console.log('✅ Copied priority4_dashboard.html to priority4.html');\"",
    "docker:frontend": "docker-compose up frontend",
    "docker:build": "docker-compose up --build"
  },
  "keywords": ["finance", "ai-budgeting", "expenses", "tracking", "react-native", "expo"],
  "author": "PCSchmidt",
  "license": "MIT",
  "dependencies": {
    "expo": "~49.0.0",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "@react-navigation/native": "^6.1.0",
    "@react-navigation/stack": "^6.3.0",
    "@react-navigation/bottom-tabs": "^6.5.0",
    "react-native-screens": "~3.22.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-gesture-handler": "~2.12.0",
    "axios": "^1.6.0",
    "react-native-elements": "^3.4.3",
    "react-native-paper": "^5.10.0",
    "victory-native": "^36.6.8",
    "react-native-vector-icons": "^10.0.0",
    "@expo/vector-icons": "^13.0.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-svg": "13.9.0",
    "expo-font": "~11.4.0",
    "expo-asset": "~8.10.1",
    "expo-constants": "~14.4.2",
    "expo-status-bar": "~1.6.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "react-native-dotenv": "^3.4.9",
    "date-fns": "^2.30.0",
    "react-native-modal": "^13.0.1",
    "react-native-picker-select": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
